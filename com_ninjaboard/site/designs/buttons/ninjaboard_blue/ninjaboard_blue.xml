	<buttonset translateable="1" sprite="sprite.png" spriteIE="sprite.gif">
		<button filename="button_addfile.png" function="buttonAddFile" x="NB_BUTTONX" hover="NB_BUTTONHOVERYADDFILE" active="NB_BUTTONACTIVEYADDFILE" y="NB_BUTTONYADDFILE" width="NB_BUTTONWADDFILE" height="NB_BUTTONSMALL" title="NB_BUTTONADDFILE" />
		<button filename="button_cancel.png" function="buttonCancel" x="NB_BUTTONX" hover="NB_BUTTONHOVERYCANCEL" active="NB_BUTTONACTIVEYCANCEL" y="NB_BUTTONYCANCEL" width="NB_BUTTONWCANCEL" height="NB_BUTTONSMALL" title="NB_BUTTONCANCEL" />
